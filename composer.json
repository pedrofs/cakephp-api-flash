{
    "name": "cakephp-api-flash",
    "description": "CakePHP 3 plugin that adds any flash message to the response.",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp", "api", "flash", "cakephp3", "flash-message"
    ],
    "homepage": "https://github.com/pedrofs/cakephp-api-flash",
    "license": "UNLICENSE",
    "authors": [
        {
            "name": "Pedro Fernandes Steimbruch",
            "email": "pedrofsteimbruch@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.45",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "CakeDone\\ApiFlash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDone\\ApiFlash\\": "src",
            "CakeDone\\ApiFlash\\Test\\": "tests",
            "CakeDone\\ApiFlash\\TestApp\\": "tests/test_app/TestApp"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}